{
  "name": "hangjegyzet-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:custom": "node server/index.js",
    "build": "next build",
    "start": "next start",
    "start:custom": "NODE_ENV=production node server/index.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:staging": "dotenv -e .env.staging prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:staging": "dotenv -e .env.staging tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:staging": "npm run test:smoke -- --env=staging",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:smoke:staging": "node scripts/smoke-tests.js staging",
    "test:smoke:production": "node scripts/smoke-tests.js production",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "analyze:both": "BUNDLE_ANALYZE=both npm run build",
    "build:profile": "NEXT_TELEMETRY_DISABLED=1 npm run build -- --profile",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:smoke": "node scripts/smoke-tests.js",
    "db:backup": "node scripts/db-backup.js",
    "deploy:staging": "./scripts/deploy-staging.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "backup:staging": "pg_dump $DATABASE_URL > backups/staging-$(date +%Y%m%d-%H%M%S).sql",
    "restore:staging": "psql $DATABASE_URL < $1",
    "dev:ws": "tsx server.ts",
    "start:ws": "node server.js",
    "workers": "tsx workers/index.ts",
    "workers:dev": "tsx --watch workers/index.ts",
    "workers:transcription": "tsx workers/transcription.worker.ts",
    "workers:ai": "tsx workers/ai-processing.worker.ts",
    "load-test:smoke": "k6 run load-tests/scenarios/smoke-test.js",
    "load-test:load": "k6 run load-tests/scenarios/load-test.js",
    "load-test:stress": "k6 run load-tests/scenarios/stress-test.js",
    "load-test:spike": "k6 run load-tests/scenarios/spike-test.js",
    "load-test:all": "npm run load-test:smoke && npm run load-test:load && npm run load-test:stress && npm run load-test:spike",
    "performance:analyze": "tsx scripts/analyze-performance.ts",
    "performance:report": "tsx scripts/generate-performance-report.ts",
    "prepare": "node -e \"if (process.env.CI !== 'true' && process.env.VERCEL !== '1') { require('child_process').execSync('husky install', { stdio: 'inherit' }) }\"",
    "pre-commit": "lint-staged"
  },
